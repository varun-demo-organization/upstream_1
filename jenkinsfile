#!/usr/bin/env groovy
properties([
   [$class: 'GithubProjectProperty',
   displayName: '',
   projectUrlStr: 'https://github.com/varun-demo-organization/upstream_1.git/'],
   pipelineTriggers([
      upstream(
  threshold: 'SUCCESS',
  upstreamProjects: 'https://github.com/varun-demo-organization/upstream_2.git/'
  ) ])])

pipeline {
   agent any

   stages {
       stage('Build') {
           steps {
               sh 'pwd'
           }
       }
       stage('Test'){
           steps {
               sh 'java -version'
               
           }
       }
       stage('Deploy') {
           steps {
               sh 'ls'
               sh 'pwd'
           }
       }
   }
}
